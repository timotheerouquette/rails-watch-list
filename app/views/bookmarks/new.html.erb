<body>
  <div class="banner">
  </div>

  <div class="container">
    <div class="card card-create">
      <h4>New bookmark:</h4>
      <%= simple_form_for [ @list, @bookmark ] do |f| %>
        <%= f.input :comment %>
        <%= f.association :movie %>
        <%= f.submit "Submit bookmark", class: "btn btn-primary"%>
      <% end %>
    </div>
  </div>
</body>
